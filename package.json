    "jest-watch-typeahead": "^0.6.3",
    "jsdom": "^16.7.0",
    "lint-staged": "^11.1.2",
    "markdown-loader": "^6.0.0",
    "prettier": "^2.4.1",
    "sass": "1.39.2",
    "sass-loader": "^10.1.1",
    "storybook-addon-jsx": "^7.3.14",
    "style-loader": "^2.0.0",
    "svgo": "2.7.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
